{
    "name": "devinci-it/login-core",
    "type": "library",
    "description": "Essential features for Laravel applications, including a standard login and registration system.",
    "keywords": ["laravel", "authentication", "login", "registration"],
    "license": "MIT",
    "authors": [
        {
            "name": "devinci-it"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Devinci\\LaravelEssentials\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "LoginCoreServiceProvider"
            ]
        }
    }
}
